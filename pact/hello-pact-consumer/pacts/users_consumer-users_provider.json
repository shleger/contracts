{
  "consumer": {
    "name": "Users Consumer"
  },
  "provider": {
    "name": "Users Provider"
  },
  "interactions": [
    {
      "description": "a request for JSON data",
      "request": {
        "method": "GET",
        "path": "/users"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "email": "Sincere@april.biz",
          "username": "Bret",
          "id": 1000
        },
        "matchingRules": {
          "$.body.email": {
            "match": "regex",
            "regex": "^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$"
          },
          "$.body.username": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}